{
  "name": "ortoo",
  "version": "0.0.1",
  "description": "Ortoo Javascript SDK",
  "scripts": {
    "dev": "webpack-dev-server --config=./config/webpack.dev.js",
    "test": "karma start",
    "coverage": "karma start --coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean": "rimraf dist coverage",
    "eslint:fix": "eslint --ext .tsx,.ts --fix ./src",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*.{json,ts,tsx}",
    "gen-protocols:protoc": "protoc -I=./src/protocols/protobuf ortoo.proto --js_out=import_style=commonjs,binary:./src/protocols/protobuf --grpc-web_out=import_style=typescript,mode=grpcweb:./src/protocols/protobuf",
    "gen-protocols:add-declare": "sed -i 's/export namespace/export declare namespace/g' src/protocols/protobuf/ortoo_pb.d.ts",
    "gen-protocols": "npm run gen-protocols:protoc && npm run gen-protocols:add-declare",
    "build": "npm run clean && webpack --config=./config/webpack.dev.js",
    "build:prod": "npm run clean:dist && webpack --config=./config/webpack.prod.js  --progress --profile --bail",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist && cp ./src/protocols/protobuf/*.js ./dist/protocols/protobuf",
    "build:js": "npx babel src --config-file=config/babelrc.config.json --out-dir dist --extensions \".ts\" --source-maps inline",
    "build:example": "npm run build:types && npm run build:js && rollup -c ./config/rollup.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowhunger/ortoo-js.git"
  },
  "author": "Ortoo Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/knowhunger/ortoo-js/issues"
  },
  "homepage": "https://github.com/knowhunger/ortoo-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/chai": "^4.2.14",
    "@types/google-protobuf": "^3.7.4",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-search-and-replace": "^1.1.0",
    "chai": "^4.2.0",
    "coverage-istanbul-loader": "^3.0.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jsdoc": "^3.6.6",
    "json-loader": "^0.5.7",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "requirejs": "^2.3.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.11",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.1"
  },
  "dependencies": {
    "google-protobuf": "^3.14.0",
    "grpc-web": "^1.2.1",
    "mqtt": "^4.2.6",
    "uuid": "^8.3.1"
  }
}
